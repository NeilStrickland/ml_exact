<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">conv := proc(A,K,b)
 local a,k,i,j,p,q;
 a := (i,j) -&gt; `if`(0 &lt;= i and i &lt; N and 0 &lt;= j and j &lt; N,A[i+1][j+1],0);
 k := (i,j) -&gt; `if`(-1 &lt;= i and i &lt;= 1 and -1 &lt;= j and j &lt;= 1,K[i+2][j+2],0);
 [seq([seq(max(0,b+add(add(a(i-p,j-q)*k(p,q),q=-1..1),p=-1..1)),j=0..N-1)],i=0..N-1)];
end:</Text-field>
</Input>
